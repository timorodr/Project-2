<!DOCTYPE html>
<html lang="en">

    <%- include("../partials/head.ejs") %>

<body>
    <div id="container">
        <header class="budget-cal-add">
            <div id="budget">
                <h2>Budget</h2>
                <h1 id="budget-dollars">$</h1> <!-- fix this-->
            </form>
            </div>    
            <div>
                <h2>Next Appointment</h2>
                <h1>04/15/2024</h1>
            </div>
                <div>
                    <a href="/tattooly/new"><button class="add-btn">+ Add a Tattoo</button></a>
                </div>
        </header>

        <aside>
            <div class="app-name">
                <img src="data:image/png;base64," class="logo">
            </div>

            <div class="profile">
                <div class="profile-photo">
                    <img src="https://media.licdn.com/dms/image/C5603AQEi7gUPi-Pu8w/profile-displayphoto-shrink_400_400/0/1652236717319?e=2147483647&v=beta&t=LUJF53pNIiKDSDrS_hq6T3GdjTMlen8HHQz4DpRHjsE" alt="tim">
                    <p>Tim Rodriguez</p>
                    <p>@NANERO</p>
                </div>
            </div>
            <div class="profile-menu">
                <div class="profile-artists">Artists</div>
                <div class="profile-calendar">
                    <button onclick="the_thing.showModal()" class="calendar-btn">Edit Calendar</button>
                    <dialog id="the_thing">
                        <h1>Edit calendar vibratinos ensuing</h1>
                        <button onclick="the_thing.close()">Put that thing back or so help me God</button>
                    </dialog>
                </div>
                <div class="profile-budget">
                    <button onclick="the_thing2.showModal()" class="budget-btn">Edit Budget</button>
                    <dialog id="the_thing2">
                        <form id="update-budget" method="post">
                            <label for="newBudget">Tattoo Budget:</label>
                            <input type="text" id="newBudget" name="budget" value="$" required>
                            <button type="button" onclick="updateBudget(); the_thing2.close();" value="Update Budget" class="update-budget-btn">Update Budget</button>
                        </form>
                        <script>
                            function updateBudget() {
                                let newBudget = document.getElementById("newBudget").value
                                document.getElementById("budget-dollars").innerText = newBudget
                                
                            }
                        </script>
                        <!-- <button onclick="the_thing2.close()">Put that thing back or so help me God</button> -->
                    </dialog>
                </div>
                <div class="profile-settings">Settings</div>
                <div class="profile-logout"><a href="/user/logout"><button>Logout</button></a></div>
            </div>
        </aside>

        <section class="art-container">
            <% for (tattoo of tattoos) { %>
            
            <div>
                <div class="index-bento">
                    <a href="/tattooly/<%= tattoo._id %>">
                    <img src="<%=tattoo.image%>" alt="<%= tattoo.description %>"> <!-- Think more through in the future-->
                    <p class="index-description"><%= tattoo.description %></p>
                    <p class="index-date"><%= tattoo.uploadDate %></p>
                </div>
                </a>
            </div>
            
            <% } %>
        </section>
    </div>
</body>
</html>